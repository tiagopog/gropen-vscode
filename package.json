{
  "name": "gropen-vscode",
  "displayName": "gropen.vscode",
  "description": "Plugin for quickly opening local files and directories in remote git repositories.",
  "icon": "images/gropen.jpeg",
  "publisher": "tiagopog",
  "version": "0.1.1",
  "repository": "https://github.com/tiagopog/gropen-vscode",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:gropen-vscode.gropen_file",
    "onCommand:gropen-vscode.gropen_current_line",
    "onCommand:gropen-vscode.gropen_line_range"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gropen-vscode.gropen_file",
        "title": "Gropen File"
      },
      {
        "command": "gropen-vscode.gropen_current_line",
        "title": "Gropen Current Line"
      },
      {
        "command": "gropen-vscode.gropen_line_range",
        "title": "Gropen Line Range"
      }
    ],
    "keybindings": [
      {
        "command": "gropen-vscode.gropen_file",
        "key": "ctrl+alt+g",
        "mac": "alt+cmd+g"
      },
      {
        "command": "gropen-vscode.gropen_current_line",
        "key": "ctrl+alt+l",
        "mac": "alt+cmd+l"
      },
      {
        "command": "gropen-vscode.gropen_line_range",
        "key": "ctrl+alt+r",
        "mac": "alt+cmd+r"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
